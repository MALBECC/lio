      COMMON/BLOC05/REDEL,DELTAT,DEL,VF(20),FFF(20),AT
      COMMON/BLOC06/FACTA,FACTV,FACT2,TEMPRQ,TEMPRQQ,TOTMASQ
      COMMON/BLOC07/EPS(20),SIGMA(20),WWM(20),WWQ,VFQ
      COMMON/BLOC08/FX(NAT),FY(NAT),FZ(NAT),RCT,RCTSQ,RKAPPA,RKAPPA2
      COMMON/BLOC09/TEMPA(20),TEMPAV,TEMTRS,TEPION,RRNIT,RM2
      COMMON/BLOC11/BXLGTH,ZZZ(20),IEWLD,ITEL,NTIME,NSCAL
      COMMON/BLOC12/CC,ETLR,VTLR,RHO,E2,TEMPSLV,TEMPSLT
      COMMON/BLOC13/VCT,VLJ,VCOO,VOO,VOH,VHH,VTOT,VLJQC,VCQC
      COMMON/BLOC14/TOTMAS,WEIGHT,NNAT(20),NWAT,NPART,NSPECQ
      COMMON/BLOC15/E12(20),F12(20),E6(20),F6(20),AZ(20),IPR1,IP15
      COMMON/BLOC16/DA(20),NCX,NCY,NCZ,NEWV,NEWQ,IPR,IMPR,ICMOT,IQMOT
      COMMON/BLOC17/GX(NAT),GY(NAT),GZ(NAT),GQ(NAT)
      COMMON/BLOC18/TOLL,VINC,MAXI5,MAXIT,TEMPS
      COMMON/BLOC19/NEIGH(30,30),COSANG,FL1,FL2,DOH
      COMMON/BLOC20/X1(NAT),Y1(NAT),Z1(NAT)
      COMMON/BLOC21/SUMF,SUMFX,SUMFY,SUMFZ
      COMMON/BLOC23/DIX(51,101),DIY(51,101),DIZ(51,101),PIP(51,101)
      COMMON/BLOC24/XX1(NAT),YY1(NAT),ZZ1(NAT)
      COMMON/BLOC25/QA,QB,AXI,BYI,CZI,IFLUC,KSQMAX
      COMMON/BLOC26/KG(8,1100),DELR,ENEFUR,GGRID,GRIL,IGRIL,IGGRID
      COMMON/BLOC27/AB,B,CB,UC,FC,AF,BF,CSHIFT,ESHIFT,RBUF,DELR2
      COMMON/BLOC28/PC0(NAT),PC(NAT),PCC(NAT),FQ(NAT),VQ0(NAT),VQ(NAT)
      COMMON/BLOC29/VSELF,OV(10),ENOH,EGP,ECSELF,FQQ(NAT),PC1(NAT)
      COMMON/BLOC30/STQ,S0Q,SDQ,SD0Q,QSQ,FFQ,AA,BB,DD
      COMMON/BLOC31/FQC(NAT),FQP(NAT),FQV(NAT)
      COMMON/BLOC32/DIPT,NDFT,NPAS
      COMMON/BLOC33/ICON,NSAVE,IRDF,NEW,IZZ,NATOM
      COMMON/BLOC34/XFAX,NANG,IUNID,CONVF,CCQ,ICLSTR
      COMMON/BLOC35/KG1(2,101,101),QK(101,101)
      COMMON/BLOC36/VCSS(NAT),VCSSO,VCSSH,VCSST,EAC(NAT),IRAS
      COMMON/BLOC37/Xmi(3,3),JNFC(NAT),rrcu(NAT),rrcuq(NAT),NWATC  
      COMMON/BLOC38/CKUM,RUM,KBER,JVEL,BERSTEP,BER1,BER2,UMBRE,US1,US2             
      COMMON/YU/X99,IDUM

      COMMON/BLOC39/EE12O,EE12H,EE6O,EE6H
      COMMON/BLOC40/FF12O,FF12H,FF6O,FF6H
      COMMON/BLOC41/FF12OH,FF6OH,EE12OH,EE6OH
