C
C     DATOS COMUNES PARA TODAS LAS RUTINAS 
C     DEL SOLVENTE (SISTEMA CLASICO)
C 
      IMPLICIT DOUBLE PRECISION (A-H,O-Z)
       INTEGER UMBRE,US1,US2
       REAL*8 CKUM,RUM,KBER,BER1,BER2,BERSTEP,JVEL                    
      PARAMETER (NAT=6000)
      PARAMETER (ZERO=0.0)
      PARAMETER (ONE=1.D0)
      PARAMETER (TWO=2.D0)
      PARAMETER (THREE=3.D0)
      PARAMETER (FOUR=4.D0)
      PARAMETER (FIVE=5.D0)
      PARAMETER (PTFIVE=0.5D0)
      PARAMETER (THIRD3=1.D0/3.D0)
      PARAMETER (PI=3.14159265358979312D0)
      PARAMETER (FPI=4.D0*PI)
      PARAMETER (TWOPI=2.D0*PI)
      PARAMETER (EE=4.80324 D02)
      PARAMETER (AVNUM=6.022045 D23)
      PARAMETER (BOLTZF=1.380662 D0)
      PARAMETER (A0=0.52918 D0)
      PARAMETER (HH=EE*EE/A0)

      CHARACTER*5, AT(NAT),CR
      COMMON/BLOC00/X(NAT),Y(NAT),Z(NAT),X0(NAT),Y0(NAT),Z0(NAT)
      COMMON/BLOC01/VX(NAT),VY(NAT),VZ(NAT),VX0(NAT),VY0(NAT),VZ0(NAT)
